{
    "name": "polntantos/annotation-plugin",
    "description": "This is my package annotation-plugin",
    "keywords": [
        "polntantos",
        "laravel",
        "annotation-plugin"
    ],
    "homepage": "https://github.com/polntantos/annotation-plugin",
    "support": {
        "issues": "https://github.com/polntantos/annotation-plugin/issues",
        "source": "https://github.com/polntantos/annotation-plugin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "polntantos",
            "email": "polykarpos97@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Polntantos\\AnnotationPlugin\\": "src/",
            "Polntantos\\AnnotationPlugin\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Polntantos\\AnnotationPlugin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Polntantos\\AnnotationPlugin\\AnnotationPluginServiceProvider"
            ],
            "aliases": {
                "AnnotationPlugin": "Polntantos\\AnnotationPlugin\\Facades\\AnnotationPlugin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}